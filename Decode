-- Hehe --
function _G.use(haha)
    local v1, v2, v3, v4, v5, v6
    function n1(n2)
        v1 = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
        n2 = string.gsub(n2, '[^' .. v1 .. '=]', '')
        return (n2:gsub('.', function(n3)
            if (n3 == '=') then return '' end
            v2, v3 = '',(v1:find(n3) - 1)
            for i = 6, 1, -1 do v2 = v2 .. (v3%2^i - v3%2^(i - 1)>0 and '1' or '0') end
            return v2;
        end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(n3)
            if (#n3 ~= 8) then return '' end
        v4 = 0
            for i = 1, 8 do v4 = v4 + (n3:sub(i, i) == '1' and 2^(8 - i) or 0) end
            return string.char(v4)
        end))
    end
    v5 = haha
    v6 = n1(v5)
    loadstring(v6)()
end
